{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "prettier",
  "plugins": ["prettier"],
  "globals": {
    "page": "readonly",
    "describe": "readonly",
    "it": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error"
  }
}
